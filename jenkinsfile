pipeline {
 agent any
 tools {
 maven 'Maven-3.8.6'
 }
 
  stages {
    stage ('initialize') {
       steps {
          sh '''
                 echo "PATH = ${path}
                 echo "M2_Home = ${M2_HOME}"
             '''
       }
     }
     stage ('build') {
        steps { 
     sh 'mvn clean package'
      }
     }
   }
 }       
                   
