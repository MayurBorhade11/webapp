pipeline {
 agent any
 tools {
 maven 'Maven'
 }
 
  stages {
    stage ('initialize') {
       steps {
          sh '''
                 echo "PATH = ${path}
                 echo "M2_Home = ${M2_HOME}"
             '''
       }
     }
     stage ('Build') {
        steps { 
     sh 'mvn clean package'
     
     }
    }
   }
 }       
                   
